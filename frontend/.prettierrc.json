{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "arrowParens": "always",
  "printWidth": 100,
  "blankLine": "always",
  "padding-line-between-statements": [
    { "blankLine": "always", "prev": "*", "next": "const" },
    { "blankLine": "always", "prev": "*", "next": "let" },
    { "blankLine": "always", "prev": "*", "next": "var" }
  ]
}
